{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "app",
  "compilerOptions": {
    "tsConfigPath": "tsconfig.build.json",
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true,
    "assets": [
      { "include": "core/i18n/**/*", "watchAssets": true }
]
  }
}
